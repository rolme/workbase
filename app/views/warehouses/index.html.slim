= render "warehouses/page_header", header: 'Warehouses', links: [:inventory_index, :add_warehouse]

.row
  .col-xs-12
    table.table.table-striped.table-hover
      thead
        tr
          th Name
          th
      tbody
        - @warehouses.each do |warehouse|
          tr data-url=warehouse_path(warehouse)
            td = warehouse.name
            td.text-right
              = fa_icon('search-plus')
